{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/health",
      "dest": "/server.js"
    },
    {
      "src": "/api/pokemon",
      "dest": "/server.js"
    },
    {
      "src": "/api/compare",
      "dest": "/server.js"
    }
  ]
}